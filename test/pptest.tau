require "print.tau";
module quux{
    public func puts(s: string);
    public func quux(){
        puts("quux :)");
    }
}
module bar{
    import quux::(quux);
    public func foo(){
        #quux();
        quux();
    }
}

import bar;
public func main() -> int{
    i:= 0;
    bar::foo();
    loop{
        printfln("foo: %i", i);
        i++;
        if(i==10)break;
    }
    return 0;
}
