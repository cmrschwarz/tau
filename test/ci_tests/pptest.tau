require "support/libc.tau";

import libc::(printf);

module quux{
    import libc::(puts);
    public func quux(){
        puts("quux :)");
    }
}
module bar{
    import quux::(quux);
    public func foo(){
        #quux();
        quux();
    }
}

import bar;
public func main() -> int{
    i:= 0;
    bar::foo();
    loop{
        printf("foo: %i\n", i);
        i++;
        if(i==10)break;
    }
    return 0;
}
